# Example environment variables for SwapIt deployment on Vercel
# Copy to .env.local and fill in values. NEVER commit real secrets.

# Public (ok to expose) RPC key (rate-limited) for Solana via Helius
NEXT_PUBLIC_HELIUS_API_KEY=your_helius_key_here
# Optional override full RPC endpoint (if set takes precedence over key)
# NEXT_PUBLIC_RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Default input/output token mints for Jupiter widget
NEXT_PUBLIC_INITIAL_INPUT_MINT=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
NEXT_PUBLIC_INITIAL_OUTPUT_MINT=So11111111111111111111111111111111111111112
NEXT_PUBLIC_INITIAL_SWAP_UI_AMOUNT=1
